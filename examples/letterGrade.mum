task letterGrade(percent):
    if percent >= 90:
        mumble("You got an A, amazing job!")
    elif percent >= 80 and percent < 90:
        mumble("You got an B, good job!")
    elif percent >= 70 and percent <80:
        mumble("You got a C, try a little harder next time")
    elif percent >= 60 and percent <70:
        mumble("You got a D, good luck next semester ")
    else percent < 60:
        mumble("You failed.... :(")
